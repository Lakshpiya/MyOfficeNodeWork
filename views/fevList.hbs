<div class="row card" style="margin-top:10px;padding-left:10px;padding-right:10px;">

<form class="omb_loginForm" action="/getfevItem" autocomplete="off" method="POST">
	<div class="input-group" style="margin-top:20px;">
		<span class="input-group-addon"><i class="fa fa-search"></i></span>
		<input type="text" class="form-control" id="track" name="track" autocomplete="off" required placeholder="Get Your Fev Item">               
        <input type="hidden" id="hdnFTI" name="hdnFTI" value=""/>
	</div>    
    <button class="btn btn-block loggedinhdbgCol" onclick="findLocalItems();" style="color:#000!important;" type="submit">Get Detail</button>
</form>

</div>


<div class="container">  
    {{#if test}}        
    <div class="row" style="margin-bottom: 60px;">
        <div class="col-12 col-md-6 col-lg-12">
            <div class="card">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col col-sm-4">Artist Name</div>
                                    <div class="col col-sm-4">Artist ID</div>
                                </div>
                            </div>
            </div>
        </div>
       {{#each test}}               
                    <div class="col-12 col-md-6 col-lg-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col col-sm-4">
                                        <a href='{{this.artistName}}'>
                                        {{this.artistName}}
                                        </a>
                                    </div>
                                    <div class="col col-sm-4">
                                        <a href='{{this.artistId}}'>
                                        {{this.artistId}}
                                        </a>
                                    </div>
                                </div>
                                
                            </div>
                        </div> 
                    </div>
        {{/each}}
    </div>
    {{else}}
    <p><em>There are currently no data.</em></p>
    {{/if}}
</div>